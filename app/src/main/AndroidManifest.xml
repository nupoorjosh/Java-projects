<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.kjsce">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <!--
    <uses-permission android:name="android.permission.PACKAGE_USAGE_STATS" />
    -->

    <application
        android:allowBackup="true"
        android:icon="@drawable/somaiya"
        android:label="KJSCE"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main2Activity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Trends"
            android:label="Trends"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ScoreBetter"
            android:label="ScoreBetter"
            android:screenOrientation="portrait" />
        <activity
            android:name=".News"
            android:label="News2"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Others"
            android:label="Others"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:label="MainActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".Profile" />
        <activity android:name=".Profile2" />
        <activity android:name=".Profile3" />
        <activity android:name=".Help"></activity>
    </application>

</manifest>